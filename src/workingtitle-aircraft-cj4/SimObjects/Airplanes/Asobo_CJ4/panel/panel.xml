	</Instrument>
	<Instrument>
		<Name>CJ4_MFD</Name>
		<Electric>
			<Or>
				<Simvar name="CIRCUIT ON:41" unit="Boolean"/>
				<Simvar name="CIRCUIT ON:42" unit="Boolean"/>
			</Or>
		</Electric>
	</Instrument>
	<Instrument>
		<Name>CJ4_SAI</Name>
		<Electric>
			<Simvar name="CIRCUIT ON:49" unit="Boolean"/>
		</Electric>
	</Instrument>
	<Instrument>
		<Name>CJ4_FMC</Name>
		<Electric>
			<Or>
				<Simvar name="CIRCUIT ON:47" unit="Boolean"/>
				<Simvar name="CIRCUIT ON:48" unit="Boolean"/>
