	</Instrument>
	<Instrument>
		<Name>CJ4_PFD</Name>
		<RadarAltitude>True</RadarAltitude>
		<Electric>
			<Or>
				<Simvar name="CIRCUIT ON:39" unit="Boolean"/>
				<Simvar name="CIRCUIT ON:40" unit="Boolean"/>
			</Or>
